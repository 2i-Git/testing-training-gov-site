{
  "name": "alcohol-license-training-app",
  "version": "1.0.0",
  "description": "Training application for testers - Alcohol License Application System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "init-db": "node scripts/init-db.js",
        "docker:build": "docker build -t alcohol-license-app .",
    "docker:run": "docker run -p 3000:3000 --env-file .env alcohol-license-app",
    "docker:compose:up": "docker compose up -d",
    "docker:compose:down": "docker compose down",
    "docker:compose:logs": "docker compose logs -f",
    "docker:compose:build": "docker compose build --no-cache",
    "restart": "npm stop --if-present && npm start"
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "govuk-frontend": "^5.11.1",
    "helmet": "^7.2.0",
    "nunjucks": "^3.2.4",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "training",
    "testing",
    "government",
    "forms",
    "api"
  ],
  "author": "Training Team",
  "license": "MIT"
}
